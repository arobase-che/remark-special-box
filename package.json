{
  "name": "remark-special-box",
  "version": "0.1.0",
  "description": "A remark plugin for Markdown that parse special blox",
  "main": "app.js",
  "scripts": {
    "start": "node app.js"
  },
  "dependencies": {
    "unist-util-visit": "^1.1.3"
  },
  "author": "ache",
  "license": "MIT",
  "keywords": [
    "box",
    "remark"
  ],
  "devDependencies": {
    "xo": "^0.18.2"
  },
  "xo" : {
    "space" : true,
    "rules": {
      "comma-dangle": [
        "error",
        "always-multiline"
      ]
    }
  }
}
